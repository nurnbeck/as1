<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="habittracker.cs.ualberta.ca.habittracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HabitTrackerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="habittracker.cs.ualberta.ca.habittracker.oldHabitTrackerActivity"
            android:label="oldHabitTrackerActivity" >
        </activity>

        <activity
            android:name="habittracker.cs.ualberta.ca.habittracker.AddHabitActivity"
            android:label="AddHabitActivity" >
        </activity>

        <activity
            android:name="habittracker.cs.ualberta.ca.habittracker.DeleteOldHabitActivity"
            android:label="DeleteOldHabitActivity" >
        </activity>

        <activity
            android:name="habittracker.cs.ualberta.ca.habittracker.delHabitActivity"
            android:label="delHabitActivity" >
        </activity>

    </application>

</manifest>